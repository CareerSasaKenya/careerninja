{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/jobs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, stale-while-revalidate=86400"
        },
        {
          "key": "Vary",
          "value": "User-Agent"
        }
      ]
    }
  ]
}